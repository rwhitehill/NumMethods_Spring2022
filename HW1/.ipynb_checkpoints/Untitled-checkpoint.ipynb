   ],
   "source": [
    "A = sp.Matrix([[2,-1,0],[-1,2,-1],[0,-1,2]])\n",
    "eigenvals = (A.T*A).eigenvals()\n",
    "eigenvals\n",
    "lam = sp.symbols('lambda')\n",
    "expr = (A.T*A - lam*sp.eye(3)).det()\n",
    "sp.solve(expr,lam)"
   ]
